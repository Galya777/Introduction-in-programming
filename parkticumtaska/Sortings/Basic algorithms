#include <iostream>
using namespace std;
const int MAX =1000;
//removing negative elements from an array
int f(int arr[], size_t size) {
	int bsize = 0;
	for (size_t i = 0; i < size; ++i) {
		if (arr[i] > 0) {
			arr[bsize] = arr[i];
			++bsize;
		}
	}
	for (size_t i = 0; i < bsize; ++i) {
		return arr[i];
	}
}
int main()
{
	

	return 0;
}
