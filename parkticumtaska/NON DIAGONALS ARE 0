#include <iostream>
const int MAX = 128;
using namespace std;
int main()
{
	int arr[MAX][MAX];
	int number,n;
	cin >> number;
	for (int i = 0; i < number; i++) {
		for (int j = 0; j < number; j++) {
			cin >> n;
			arr[i][j] = n;
		}
	} 
	for (int i = 0; i < number; i++) {
		for (int j = 0; j < number; j++) {
			if (i != j && i + j != number - 1) {
				arr[i][j] = 0;
			}
		}
	} 
	for (int i = 0; i < number; i++) {
		for (int j = 0; j < number; j++) {
			cout << arr[i][j]<<" ";
			if (j == number - 1) {
				cout << endl;
			}
		}
	}
	return 0;
}
