#include <iostream>
using namespace std;
const int MAX = 128;
int main()
{
	int num, n;
	int vectors[MAX];
	int vectors2[MAX];
	cin >> num;
	for (int i = 0; i < num; i++) {
		cin >> n;
		vectors[i] = n;
	} for (int i = 0; i < num; i++) {
		cin >> n;
		vectors2[i] = n;
	}int scalar = 0;
	for (int i = 0; i < num; i++) {
		scalar += vectors[i] * vectors2[i];
	}
	cout << scalar << endl;
	return 0;
}
